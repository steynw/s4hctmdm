COLUMN TABLE "EventValidationConfig" (
     "ErrorCode" NVARCHAR(1) NOT NULL COMMENT 'Error Code',
	 "ErrorDesc" NVARCHAR(60) COMMENT 'Error Desc',
	 "SuggestedAction" NVARCHAR(50) COMMENT 'Suggested Action',
